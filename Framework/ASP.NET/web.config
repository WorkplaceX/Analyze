<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <!--
    Configure your application settings in appsettings.json. Learn more at http://go.microsoft.com/fwlink/?LinkId=786380
  -->

  <system.webServer>
    <handlers>
      <add name="iisnode" path="Application/Nodejs/Server/AngularUniversalServerNoSpa.js" verb="*" modules="iisnode"/>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="AngularUniversalServerNoSpa">
          <match url="Angular/AngularUniversalServerNoSpa" />
          <action type="Rewrite" url="Application/Nodejs/Server/AngularUniversalServerNoSpa.js"/>
        </rule>
      </rules>
    </rewrite>
    <aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" forwardWindowsAuthToken="false"/>
  </system.webServer>
</configuration>
